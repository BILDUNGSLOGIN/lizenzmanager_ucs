export LC_ALL=C.UTF-8

%:
	dh $@

override_dh_auto_test:
	ucslint
	dh_auto_test

override_dh_strip_nondeterminism:
	dh_strip_nondeterminism --exclude=openapi-generator-cli-5.0.0.jar

override_dh_auto_build:
	wget -c https://repo1.maven.org/maven2/org/openapitools/openapi-generator-cli/5.0.0/openapi-generator-cli-5.0.0.jar
	py3clean .
	dh_auto_build

override_dh_auto_clean:
	dh_auto_clean
	rm -f openapi-generator-cli-5.0.0.jar
