#: src/univention/bildungslogin/utils.py:46
msgid "Assigned"
msgstr "Zugewiesen"

#: src/univention/bildungslogin/utils.py:48
msgid "Available"
msgstr "Verfügbar"

#: src/univention/bildungslogin/handlers.py:589
msgid ""
"Error creating assignment for {license_code!r}!\n"
"{exc!s}"
msgstr ""
"Fehler beim Zuweisen für die Lizenz {license_code!r}!\n"
"{exc!s}"

#: src/univention/bildungslogin/handlers.py:90
msgid ""
"Error creating license {license_code!r}!\n"
"{exc}"
msgstr ""
"Fehler beim anlegen der Lizenz {license_code!r}!\n"
"{exc}"

#: src/univention/bildungslogin/handlers.py:433
msgid ""
"Error creating meta data for product id {p_id!r}!\n"
"{exc}"
msgstr ""
"Fehler beim Erstellen von Metadaten für das Produkt mit der ID {p_id!r}!\n"
"{exc}"

#: src/univention/bildungslogin/handlers.py:788
msgid "Illegal status change to {status!r}!"
msgstr "Ungültiger Statusübergang zu {status!r}!"

#: src/univention/bildungslogin/handlers.py:821
#, python-brace-format
msgid ""
"Invalid assignment status change!\n"
"{exc}"
msgstr ""
"Ungültige Änderung des Zuweisungsstatus!\n"
"{exc}"

#: src/univention/bildungslogin/handlers.py:560
#, python-brace-format
msgid "License can't be assigned to user in schools {ous}!"
msgstr "Lizenz kann nicht an User in Schulen {ous} zugewiesen werden!"

#: src/univention/bildungslogin/handlers.py:615
msgid "License is 'ignored for display' and thus can't be assigned!"
msgstr ""
"Die Lizenz ist auf 'In Ansicht ignorieren' gesetzt und kann daher nicht "
"zugewiesen werden!"

#: src/univention/bildungslogin/handlers.py:620
msgid "License is expired and thus can't be assigned!"
msgstr "Die Lizenz ist abgelaufen und kann nicht mehr zugewiesen werden!"

#: src/univention/bildungslogin/handlers.py:155
msgid "License with code {license_code!r} does not exist."
msgstr "Lizenz mit Code {license_code!r} existiert nicht."

#: src/univention/bildungslogin/handlers.py:607
msgid ""
"License with special type 'Lehrer' can't be assigned to user {username!r} "
"with roles {roles!r}!"
msgstr ""
"Die Lizenz mit dem Sondertyp 'Lehrer' kann dem User {username!r} mit den "
"Rollen {roles!r} nicht zugewiesen werden!"

#: src/univention/bildungslogin/handlers.py:491
msgid "Meta data object with product id {p_id!r} does not exist!"
msgstr "Metadaten Objekt mit Produkt ID {p_id!r} existiert nicht!"

#: src/univention/bildungslogin/handlers.py:768
msgid "No assignment for license with {dn!r} was found for user {username!r}."
msgstr ""
"Keine Zuweisung für die Lizenz {dn!r} wurde für den User {username!r} "
"gefunden."

#: src/univention/bildungslogin/handlers.py:655
msgid ""
"No assignment left of license with code {license_code!r}. Failed to assign "
"{username!r}!"
msgstr ""
"Es sind keine Zuweisungen für die Lizenz mit dem Code {license_code!r} mehr "
"verfügbar. Dem Nutzer {username!r} wurde keine Lizenz zugewiesen!"

#: src/univention/bildungslogin/handlers.py:577
msgid "No license with code {license_code!r} was found!"
msgstr "Lizenz mit Code {license_code!r} nicht gefunden!"

#: src/univention/bildungslogin/handlers.py:567
msgid "No user with username {un!r} exists!"
msgstr "Es existiert kein User mit Username {un!r}!"

#: src/univention/bildungslogin/utils.py:47
msgid "Provisioned"
msgstr "Provisioniert"

#: src/univention/bildungslogin/utils.py:60
msgid "Single license"
msgstr "Einzellizenz"

#: src/univention/bildungslogin/handlers.py:511
msgid "There is no license for the assignment {dn!r}!"
msgstr "Es existiert keine Lizenz für die Zuweisung {dn!r}!"

#: src/univention/bildungslogin/utils.py:59
msgid "Volume license"
msgstr "Volumenlizenz"

